{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "ext-simplexml": "^7.0",
    "dingo/api": "1.0.*@dev",
    "elasticsearch/elasticsearch": "^2.",
    "flow/jsonpath": "^0.3.4",
    "laravel/framework": "5.4.*",
    "laravel/passport": "dev-master",
    "league/pipeline": "^0.3.0",
    "prettus/l5-repository": "~2.6",
    "predis/predis": "^1.1",
    "intervention/image": "~2.",
    "wanggchongg/aliyun-dysms-php-sdk":"dev-master",
    "jenssegers/mongodb": "dev-master"
  },
  "require-dev": {
    "codeception/codeception": "^2.3",
    "fzaninotto/faker": "~1.4",
    "laravel/tinker": "~1.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "rap2hpoutre/laravel-log-viewer": "^0.7.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Helpers/Helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "wanggchongg/aliyun-dysms-php-sdk": {
      "type": "git",
      "url": "https://github.com/wanggchongg/aliyun-dysms-php-sdk.git"
    },
    "jenssegers/mongodb": {
      "type" : "git",
      "url" : "https://github.com/Tucker-Eric/laravel-mongodb.git"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "secure-http": false
  }
}
